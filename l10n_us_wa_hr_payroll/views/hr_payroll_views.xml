<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="hr_contract_form_l10n_us_wa_inherit" model="ir.ui.view">
            <field name="name">hr.contract.form.inherit</field>
            <field name="model">hr.contract</field>
            <field name="priority">148</field>
            <field name="inherit_id" ref="hr_contract.hr_contract_view_form"/>
            <field name="arch" type="xml">
            <data>
                <xpath expr="//group[@name='state_filing']" position="inside">
                    <group string="Washington" name="wa">
                        <field name="wa_lni" string="LNI"/>
                    </group>
                </xpath>
            </data>
            </field>
        </record>
    </data>
</odoo>
